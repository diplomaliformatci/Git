package Grafik2_6;

import java.awt.event.ActionListener;

import javax.swing.JButton;

public class JButtonn extends JButton {
					public JButtonn(int x , int y) {
						initUI(x,y);
					}
					private void initUI(int x, int y){
						this.setLayout(null);
						this.setLocation(x,y);
						this.setSize(100, 100);
					}
					
}
